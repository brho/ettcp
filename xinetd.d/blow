# default: off
# description: An firehose server. This is the tcp \
# version.

service blow
{
	type		= UNLISTED
	id		= blow-stream
	server		= /usr/local/bin/blow
	port		= 5501
	socket_type	= stream
	protocol	= tcp
	user		= nobody
	wait		= no
	disable		= no
}                                                                               
